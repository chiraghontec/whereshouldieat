{
  "name": "foodiefind-mobile",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace FoodieFindRN.xcworkspace -scheme FoodieFindRN -configuration Release -destination generic/platform=iOS -archivePath FoodieFindRN.xcarchive archive"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.19.3",
    "@react-native-community/geolocation": "latest",
    "@react-navigation/bottom-tabs": "latest",
    "@react-navigation/native": "latest",
    "@react-navigation/native-stack": "latest",
    "react": "^19",
    "react-native": "latest",
    "react-native-fast-image": "latest",
    "react-native-gesture-handler": "latest",
    "react-native-leaflet-view": "^0.1.2",
    "react-native-paper": "latest",
    "react-native-reanimated": "^3.5.4",
    "react-native-safe-area-context": "latest",
    "react-native-screens": "^3.25.0",
    "react-native-super-grid": "^4.9.7",
    "react-native-vector-icons": "latest",
    "react-native-webview": "latest",
    "lucide-react-native": "^0.454.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^19",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=16"
  }
}
